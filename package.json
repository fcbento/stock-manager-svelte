{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public -s",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile public/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile public/smui-dark.css -i src/theme/dark"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@smui/button": "^6.0.0-beta.15",
    "@smui/card": "^6.0.0-beta.15",
    "@smui/data-table": "^6.0.0-beta.15",
    "@smui/layout-grid": "^6.0.0-beta.15",
    "@smui/tab": "^6.0.0-beta.15",
    "@smui/tab-bar": "^6.0.0-beta.15",
    "@smui/textfield": "^6.0.0-beta.15",
    "rollup": "^2.3.4",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "smui-theme": "^6.0.0-beta.15",
    "svelte": "^3.0.0"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "bootstrap": "^4.5.3",
    "material-design-icons": "^3.0.1",
    "sirv-cli": "^1.0.0",
    "svelte-routing": "^1.4.2"
  }
}
